{
  "name": "Go EToolsE framework",
  "dockerFile": "Dockerfile",
  "features": {
    "ghcr.io/devcontainers/features/node:1": {}
  },
  "onCreateCommand": {
    "install_dependencies": "go get"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "golang.go",
        "766b.go-outliner",
        "ms-azuretools.vscode-docker",
        "github.vscode-github-actions",
        "alexkrechik.cucumberautocomplete",
        "vue.volar"
      ]
    }
  },
  "forwardPorts": [5173]
}
